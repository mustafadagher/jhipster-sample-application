<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity PaymentGroup.
    -->
    <changeSet id="20181012141630-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="author_id"
                                 baseTableName="payment_group"
                                 constraintName="fk_payment_group_author_id"
                                 referencedColumnNames="id"
                                 referencedTableName="member"/>

        <addForeignKeyConstraint baseColumnNames="payment_groups_id"
                                 baseTableName="payment_group_member"
                                 constraintName="fk_payment_group_member_payment_groups_id"
                                 referencedColumnNames="id"
                                 referencedTableName="payment_group"/>
        <addForeignKeyConstraint baseColumnNames="members_id"
                                 baseTableName="payment_group_member"
                                 constraintName="fk_payment_group_member_members_id"
                                 referencedColumnNames="id"
                                 referencedTableName="member"/>
        
    </changeSet>
</databaseChangeLog>
