<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity PaymentRequest.
    -->
    <changeSet id="20181012141632-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="from_id"
                                 baseTableName="payment_request"
                                 constraintName="fk_payment_request_from_id"
                                 referencedColumnNames="id"
                                 referencedTableName="member"/>

        <addForeignKeyConstraint baseColumnNames="to_id"
                                 baseTableName="payment_request"
                                 constraintName="fk_payment_request_to_id"
                                 referencedColumnNames="id"
                                 referencedTableName="member"/>

        <addForeignKeyConstraint baseColumnNames="group_id"
                                 baseTableName="payment_request"
                                 constraintName="fk_payment_request_group_id"
                                 referencedColumnNames="id"
                                 referencedTableName="payment_group"/>

    </changeSet>
</databaseChangeLog>
